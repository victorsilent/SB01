# target programming lamguage
language: java

# JDK versios support
jdk:
  - oraclejdk7
  - oraclejdk8

sudo: required

services:
  - postgresql

# run tests, findbugs, pmd and friends using Ant, Maven or Gradle
script: ant test

before_script:
  - psql -c "create database bdvendas;" -U postgres
  - psql -c "CREATE SCHEMA trabalho" -U postgres; fi
